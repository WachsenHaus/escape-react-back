<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.react.escape.review.dto.ReviewDto" alias="reviewDto"/>
		<typeAlias type="com.react.escape.notice.dto.NoticeDto" alias="noticeDto"/>
		<typeAlias type="com.react.escape.timetable.dto.TimetableDto" alias="timetableDto"/>
		<typeAlias type="com.react.escape.timetable.dto.ResInfoDto" alias="resinfoDto"/>

	</typeAliases>

	<mappers>
		<mapper resource="com/react/escape/mybatis/ReviewMapper.xml"/>
		<mapper resource="com/react/escape/mybatis/NoticeMapper.xml"/>
		<mapper resource="com/react/escape/mybatis/TimetableMapper.xml"/>
	</mappers>		
</configuration>
